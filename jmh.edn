{:benchmarks
 [{:name :process-generation
   :fn clojush.bench.helpers/process-generation-deserialize
   :args [:state/process-generation-input]
   :apply true
   :options [{:mode :single-shot
              :measurement 5
              :warmup 5
              :threads 1
              :fork 1
              :output-time-unit :s}]}
  {:name :eval-push
   :fn clojush.interpreter/eval-push
   :args [:state/eval-push-input]
   :apply true
   :options [{:mode :single-shot
              :measurement
              {:count 100
               :iterations 50}
              :warmup
              {:count 100
               :iterations 50}
              :threads 20
              :fork 1
              :output-time-unit :ms}]}]

